{
  "name": "typetree",
  "homepage": "https://anlbrain.com/",
  "author": "Lane Sun <lanesun@anlbrain.com> (https://lanesun.neocities.org/)",
  "version": "0.1.0",
  "main": "main.js",
  "build": {
    "productName": "TypeTree",
    "appId": "com.anlbrain.typetree",
    "icon": "icon.png",
    "linux": {
      "category": "Utility",
      "icon": "res/icons",
      "target": "rpm"
    },
    "win": {
      "target": "nsis",
      "icon": "res/icon.ico"
    },
    "fileAssociations": [
      {
        "ext": "typetree",
        "name": "TypeTree File",
        "role": "Editor",
        "mimeType": "application/x-typetree"
      },
      {
        "ext": "json",
        "name": "JSON File",
        "role": "Editor",
        "mimeType": "application/json"
      }
    ]
  },
  "scripts": {
    "start": "electron .",
    "package": "electron-builder"
  },
  "type": "module",
  "devDependencies": {
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "smooth-scrollbar": "^8.8.4"
  }
}
